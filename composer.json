{
    "name": "veksoftware/yii2-report-manager",
    "description": "Extension for generation reports and setting plans",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","report manager"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Okatov",
            "email": "sokatov@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "bburim/daterangepicker": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "reportmanager\\": ""
        }
    },
    "repositories":[
        {
            "type": "package",
            "package": {
                "name": "bburim/daterangepicker",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/bburim/daterangepicker.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "extra": {
        "bootstrap": "reportmanager\\base\\Init"
    }
}
